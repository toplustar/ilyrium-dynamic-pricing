{
  "name": "ilyrium-rpc-backend",
  "version": "1.0.0",
  "description": "NestJS RPC Proxy Backend for Solana Dynamic Pricing System",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "build": "nest build",
    "build:strict": "npm run lint && npm run typecheck && nest build",
    "format": "prettier --write \"{src,eslint-rules,migrations}/**/*.{ts,js}\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "prestart:dev": "lsof -ti:3000 | xargs kill -9 2>/dev/null || true",
    "lint": "eslint \"{src,apps,libs,test,seeds,scripts}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test,seeds,scripts}/**/*.ts\" --fix",
    "lint:verbose": "eslint \"{src,apps,libs,test,seeds,scripts}/**/*.ts\" --format=stylish --debug",
    "ci:check": "npm run lint && npm run typecheck",
    "test": "echo \"No tests yet\" && exit 0",
    "test:watch": "echo \"No tests yet\" && exit 0",
    "test:cov": "echo \"No tests yet\" && exit 0",
    "db:migration:create": "typeorm-ts-node-commonjs migration:create",
    "db:migration:run": "typeorm-ts-node-commonjs migration:run -d datasource.config.ts",
    "db:migration:revert": "typeorm-ts-node-commonjs migration:revert -d datasource.config.ts",
    "db:drop": "typeorm-ts-node-commonjs schema:drop -d datasource.config.ts",
    "cache:clear": "redis-cli -h ${REDIS_HOST:-localhost} -p ${REDIS_PORT:-6379} ${REDIS_PASSWORD:+-a $REDIS_PASSWORD} FLUSHDB"
  },
  "dependencies": {
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.4.4",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.4.4",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.4",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/typeorm": "^10.0.2",
    "@solana/spl-token": "^0.4.14",
    "@solana/web3.js": "^1.98.4",
    "@tirke/node-cache-manager-ioredis": "^3.6.0",
    "@upstash/redis": "^1.35.5",
    "applicationinsights": "^2.9.5",
    "axios": "^1.7.7",
    "bcryptjs": "^3.0.2",
    "cache-manager": "^5.7.6",
    "cache-manager-ioredis-yet": "^2.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.8.1",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "nanoid": "^5.1.6",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.13.0",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "telegraf": "^4.16.3",
    "typeorm": "^0.3.20",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^10.1.4",
    "@nestjs/testing": "^10.4.4",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.7.4",
    "@types/passport-jwt": "^4.0.1",
    "@types/pg": "^8.11.10",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "nodemon": "^3.1.10",
    "prettier": "^3.3.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "{src,apps,libs,test,seeds,scripts}/**/*.ts": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "engines": {
    "node": ">=24.6.0",
    "npm": ">=10.0.0"
  }
}
